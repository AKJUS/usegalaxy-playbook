[program:{{ galaxy_host_codename }}_impersonate]
command         = /usr/bin/sg {{ galaxy_group }} '{{ galaxy_web_venv_dir }}/bin/python ./scripts/paster.py serve {{ galaxy_config_dir }}/impersonate.ini --server-name=%(program_name)s --pid-file=/home/{{ galaxy_user }}/run/%(program_name)s.pid --log-file=/home/{{ galaxy_user }}/log/%(program_name)s.log'
directory       = {{ galaxy_web_server_dir }}
process_name    = impersonate
umask           = 022
autostart       = true
autorestart     = true
startsecs       = 30
user            = {{ galaxy_user }}
environment     = PYTHON_EGG_CACHE=/home/{{ galaxy_user }}/.python-eggs
