{% else %}
{% set short_tool_id = tool_id %}
{% endif %}
{% if short_tool_id in galaxy_multicore_tools %}
{% if short_tool_id in galaxy_pulsar_incompatible_tools %}
  {{ tool_id }}: {destination: slurm_multi_direct, login_required: true}
{% else %}
